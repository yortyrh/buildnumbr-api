{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/dynamodbsample1/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/dynamodbsample1/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/dynamodbsample1",
  "projects": {
    "dynamodbsample1": {
      "type": "application",
      "root": "apps/dynamodbsample1",
      "entryFile": "main",
      "sourceRoot": "apps/dynamodbsample1/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dynamodbsample1/tsconfig.app.json"
      }
    },
    "common": {
      "type": "application",
      "root": "apps/common",
      "entryFile": "main",
      "sourceRoot": "apps/common/src",
      "compilerOptions": {
        "tsConfigPath": "apps/common/tsconfig.app.json"
      }
    }
  }
}