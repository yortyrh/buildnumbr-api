{
  "name": "sitewebr-build-counter-api",
  "version": "1.0.0",
  "description": "",
  "author": "Yorty Ruiz <yorty.ruiz@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build --builder webpack --webpackPath webpack.config.js",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch | pino-pretty",
    "start:debug": "nest start --debug --watch | pino-pretty",
    "start:prod": "node dist/apps/dynamodbsample1/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/dynamodbsample1/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/terminus": "^10.2.0",
    "aws-lambda": "^1.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "nestjs-pino": "^4.0.0",
    "pino-http": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "serverless-dynamodb-autoscaling": "^0.6.2",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/aws-lambda": "^8.10.131",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.2.2",
    "prettier-plugin-organize-imports": "^3.2.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "cobertura",
      "text-summary"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testResultsProcessor": "jest-junit",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      "test-config",
      "interfaces",
      "jestGlobalMocks.ts",
      ".module.ts",
      ".config.ts",
      "src/main.ts",
      "test",
      ".mock.ts"
    ],
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
